{
  "name": "server-base",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "nodemon -e ts --exec ts-node -r tsconfig-paths/register bin/www.ts",
    "start:dev": "cross-env NODE_ENV=development npm run start",
    "start:test": "cross-env NODE_ENV=testing npm run start",
    "start:prod": "cross-env NODE_ENV=production npm run start",
    "compile:dev": "cross-env NODE_ENV=development node bin/www.js",
    "compile:test": "cross-env NODE_ENV=testing node bin/www.js",
    "compile:prod": "cross-env NODE_ENV=production node bin/www.js",
    "pm2start:dev": "cross-env NODE_ENV=development pm2 start bin/pm2/dev.config.js",
    "pm2start:test": "cross-env NODE_ENV=testing pm2 start bin/pm2/test.config.js",
    "pm2start:prod": "cross-env NODE_ENV=production pm2 start bin/pm2/prod.config.js",
    "build": "npm run rm && npm run lint && tsc && npm run copy",
    "lint": "eslint --ext .ts",
    "lint:fix": "eslint --ext .ts --fix",
    "copy": "ts-node script/copy.ts",
    "rm": "ts-node script/rm.ts",
    "test:local": "cross-env NODE_ENV=test_local PROJECT_STATUS=source jest --runInBand --detectOpenHandles --passWithNoTests --colors --forceExit",
    "test:remote": "cross-env NODE_ENV=test_remote jest --runInBand --detectOpenHandles --passWithNoTests --colors --forceExit",
    "prepare": "husky install"
  },
  "dependencies": {
    "ajv": "^8.12.0",
    "axios": "^1.2.3",
    "cookie": "^0.5.0",
    "crypto": "^1.0.1",
    "debug": "^4.3.4",
    "dotenv": "^16.0.3",
    "ejs": "^3.1.7",
    "glob": "^8.1.0",
    "jsonwebtoken": "^9.0.0",
    "koa": "^2.14.1",
    "koa-bodyparser": "^4.3.0",
    "koa-convert": "^2.0.0",
    "koa-json": "^2.0.2",
    "koa-jwt": "^4.0.4",
    "koa-logger": "^3.2.1",
    "koa-router": "^12.0.0",
    "koa-static": "^5.0.0",
    "koa-views": "^8.0.0",
    "koa2-cors": "^2.0.6",
    "lodash": "^4.17.21",
    "mongoose": "^6.8.4",
    "mysql2": "^3.0.1",
    "redis": "^4.5.1",
    "require-all": "^3.0.0",
    "sequelize": "^6.28.0",
    "shelljs": "^0.8.5",
    "simple-git": "^3.16.0",
    "supertest": "^6.3.3",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.4.2",
    "@commitlint/config-conventional": "^17.4.2",
    "@types/cookie": "^0.5.1",
    "@types/debug": "^4.1.7",
    "@types/jest": "^29.2.6",
    "@types/jsonwebtoken": "^9.0.1",
    "@types/koa": "^2.13.5",
    "@types/koa-bodyparser": "^4.3.10",
    "@types/koa-json": "^2.0.20",
    "@types/koa-logger": "^3.1.2",
    "@types/koa-router": "^7.4.4",
    "@types/koa-static": "^4.0.2",
    "@types/koa2-cors": "^2.0.2",
    "@types/lodash": "^4.14.191",
    "@types/require-all": "^3.0.3",
    "@types/shelljs": "^0.8.11",
    "@types/supertest": "^2.0.12",
    "@types/uuid": "^9.0.1",
    "@typescript-eslint/eslint-plugin": "^5.48.2",
    "@typescript-eslint/parser": "^5.48.2",
    "cross-env": "^7.0.3",
    "eslint": "^8.32.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.1.1",
    "husky": "^8.0.3",
    "lint-staged": "^13.1.0",
    "nodemon": "^2.0.20",
    "prettier": "^2.8.3",
    "prettier-eslint": "^15.0.1",
    "shelljs": "^0.8.5",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.2",
    "typescript": "^4.9.4"
  },
  "lint-staged": {
    "**/*.ts": "npm run lint"
  }
}
